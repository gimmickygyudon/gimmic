{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ 
      {
      "source": "**",
      "destination": "/index.html"
    } 
  ],
    "headers": [
      {
        "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css|js.gz|jsgz|gz|js)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      } 
    ]
  }
}